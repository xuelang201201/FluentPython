#!/home/charlie/Python/Code/FluentPython/17_使用future处理并发/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'vaurienclient==1.1','console_scripts','vaurienctl'
__requires__ = 'vaurienclient==1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vaurienclient==1.1', 'console_scripts', 'vaurienctl')()
    )
